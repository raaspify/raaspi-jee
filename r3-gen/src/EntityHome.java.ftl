     public String escapeUniJava(String input){
      StringBuilder b = new StringBuilder();

      for (char c : input.toCharArray()) {
        if (c >= 128)
            b.append("\\u").append(String.format("%04X", (int) c));
        else
            b.append(c);
      }

      return b.toString();

     }

