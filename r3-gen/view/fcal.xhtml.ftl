<script src='./js/moment.min.js'></script>
<script src='./js/fullcalendar.min.js'></script>
<script>
$(document).ready(function() {
 $('#calendar').fullCalendar({
  header:
  {
   left: 'prev,next today',
   center: 'title',
   right: 'month,agendaWeek,agendaDay,listYear'
  },
    <#if menuAttributes2?substring(6,8) == '52'>
     height:300,
    <#else>
     height:400,
    </#if>
    defaultView: 'listYear',
    navLinks: true, // can click day/week names to navigate views
    editable: true,
    eventLimit: true, // allow "more" link when too many events
    eventTextColor:"black",
    eventBackgroundColor:"AliceBlue",
    <#if (menuAttributes2?substring(6,8) == '89' | menuAttributes2?substring(6,8) == '9a') >
    selectable: true,
    selectHelper: true,
    select: function(start, end) {
    var title = prompt('Title:');
    var eventData;
    if (title) {
     eventData = {
               title: title,
                color:"red",
                url: '/${entityName?substring(eL)}Edit.wflow?descriptionValue='+title+'&amp;startValue='+
                 start.toDate().getFullYear()+'-'+(start.toDate().getMonth()+1)+'-'+end.toDate().getDate()+' 09',
                allDay:false,
                start: start,
                end: end
                };
         $('#calendar').fullCalendar('renderEvent', eventData, true); // stick? = true
    }
         $('#calendar').fullCalendar('unselect');
  },
    </#if>
    events: function(start, end, timezone, callback) {
           // alert("Tip: click on a date " );xxxCalendarService.java has path rest/xxx/t
           // if 89 one parent one child use parent sid

        $.ajax({
            url:'rest/${componentName?substring(eL)}t',
            beforeSend: function(xhr) {
             //alert ('jay before send');
             xhr.setRequestHeader('Authorization', 'Basic ' + btoa(unescape(encodeURIComponent('clientId' + ':' + 'restPW'))))
            },
            success: function(result) {
                var events = [];
                $(result).find('root').each(function() {
                     $(this).children().each(function(){
                       events.push({
                        <#if menuAttributes2?substring(6,8) != '52'>
                         <#if menuAttributes2?substring(6,8) == '89'>
                          url: '/${entityName?substring(eL)}.wflow?${componentName}A0xxuobxbxxxxxxxxxxxsid='+$(this).children("ItemUrl").text(),
                         <#else>
                          url: '/${entityName?substring(eL)}.wflow?${componentName}A0xxuobxbxxxxxxxxxxxsid='+$(this).children("ItemUrl").text(),
                         </#if>
                        </#if>
                        title: $(this).children("Item").text(),
                        allDay:false,
                        start: $(this).children("Value").text()
                       });
                     });
                });
                callback(events);
            },
    error: function (xhr, status, error) {
     //   alert(xhr+"-j- "+status+"-j- "+error);
    },
           complete: function (xhr,status) {
            // Handle the complete event
           }
        });
    }
 });
});


</script>


